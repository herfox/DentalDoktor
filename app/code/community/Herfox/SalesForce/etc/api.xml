                        <acl>salesforce/price/info</acl>
                    </info>
                    <add translate="title" module="herfox_salesforce">
                        <title>Add precio</title>
                        <acl>salesforce/price/add</acl>
                    </add>
                    <update translate="title" module="herfox_salesforce">
                        <title>Update precio</title>
                        <acl>salesforce/price/update</acl>
                    </update>
                    <remove translate="title" module="herfox_salesforce">
                        <title>Remove precio</title>
                        <acl>salesforce/price/remove</acl>
                    </remove>
                </methods>
                <faults module="herfox_salesforce">
                    <price_not_exists>
                        <code>101</code>
                        <message>Requested precio does not exist.</message>
                    </price_not_exists>
                    <invalid_data>
                        <code>102</code>
                        <message>Provided data is invalid.</message>
                    </invalid_data>
                    <save_error>
                        <code>103</code>
                        <message>Error while saving precio. Details in error message.</message>
                    </save_error>
                    <remove_error>
                        <code>104</code>
                        <message>Error while removing precio. Details in error message.</message>
                    </remove_error>
                </faults>
            </salesforce_price>
            <salesforce_discount translate="title" module="herfox_salesforce">
                <title>Descuento API</title>
                <model>herfox_salesforce/discount_api</model>
                <acl>salesforce/discount</acl>
                <methods>
                    <list translate="title" module="herfox_salesforce">
                        <title>Retrieve list of descuentos</title>
                        <method>items</method>
                        <acl>salesforce/discount/list</acl>
                    </list>
                    <info translate="title" module="herfox_salesforce">
                        <title>Retrieve descuento info</title>
                        <acl>salesforce/discount/info</acl>
                    </info>
                    <add translate="title" module="herfox_salesforce">
                        <title>Add descuento</title>
                        <acl>salesforce/discount/add</acl>
                    </add>
                    <update translate="title" module="herfox_salesforce">
                        <title>Update descuento</title>
                        <acl>salesforce/discount/update</acl>
                    </update>
                    <remove translate="title" module="herfox_salesforce">
                        <title>Remove descuento</title>
                        <acl>salesforce/discount/remove</acl>
                    </remove>
                </methods>
                <faults module="herfox_salesforce">
                    <discount_not_exists>
                        <code>101</code>
                        <message>Requested descuento does not exist.</message>
                    </discount_not_exists>
                    <invalid_data>
                        <code>102</code>
                        <message>Provided data is invalid.</message>
                    </invalid_data>
                    <save_error>
                        <code>103</code>
                        <message>Error while saving descuento. Details in error message.</message>
                    </save_error>
                    <remove_error>
                        <code>104</code>
                        <message>Error while removing descuento. Details in error message.</message>
                    </remove_error>
                </faults>
            </salesforce_discount>
        </resources>
        <resources_alias>
            <prospect>salesforce_prospect</prospect>
            <product>salesforce_product</product>
            <price>salesforce_price</price>
            <discount>salesforce_discount</discount>
        </resources_alias>
        <v2>
            <resources_function_prefix>
                <prospect>salesforceProspect</prospect>
                <product>salesforceProduct</product>
                <price>salesforcePrice</price>
                <discount>salesforceDiscount</discount>
            </resources_function_prefix>
        </v2>
        <acl>
            <resources>
                <herfox_salesforce translate="title" module="herfox_salesforce">
                    <title>SalesForce</title>
                    <prospect translate="title" module="herfox_salesforce">
                        <title>Prospecto</title>
                        <sort_order>0</sort_order>
                        <list translate="title" module="herfox_salesforce">
