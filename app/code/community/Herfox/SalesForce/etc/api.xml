                        </remove>
                    </price>
                    <discount translate="title" module="herfox_salesforce">
                        <title>Descuento</title>
                        <sort_order>30</sort_order>
                        <list translate="title" module="herfox_salesforce">
                            <title>List</title>
                        </list>
                        <info translate="title" module="herfox_salesforce">
