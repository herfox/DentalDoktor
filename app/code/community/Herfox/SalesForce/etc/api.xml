<config>
    <api>
        <resources>
            <salesforce_prospect translate="title" module="herfox_salesforce">
                <title>Prospecto API</title>
                <model>herfox_salesforce/prospect_api</model>
                <acl>salesforce/prospect</acl>
                <methods>
                    <list translate="title" module="herfox_salesforce">
                        <title>Retrieve list of prospectos</title>
                        <method>items</method>
                        <acl>salesforce/prospect/list</acl>
                    </list>
                    <info translate="title" module="herfox_salesforce">
                        <title>Retrieve prospecto info</title>
                        <acl>salesforce/prospect/info</acl>
                    </info>
                    <add translate="title" module="herfox_salesforce">
                        <title>Add prospecto</title>
                        <acl>salesforce/prospect/add</acl>
                    </add>
                    <update translate="title" module="herfox_salesforce">
                        <title>Update prospecto</title>
                        <acl>salesforce/prospect/update</acl>
                    </update>
                    <remove translate="title" module="herfox_salesforce">
                        <title>Remove prospecto</title>
                        <acl>salesforce/prospect/remove</acl>
                    </remove>
                </methods>
                <faults module="herfox_salesforce">
                    <prospect_not_exists>
                        <code>101</code>
                        <message>Requested prospecto does not exist.</message>
                    </prospect_not_exists>
                    <invalid_data>
                        <code>102</code>
                        <message>Provided data is invalid.</message>
                    </invalid_data>
                    <save_error>
                        <code>103</code>
                        <message>Error while saving prospecto. Details in error message.</message>
                    </save_error>
                    <remove_error>
                        <code>104</code>
                        <message>Error while removing prospecto. Details in error message.</message>
                    </remove_error>
                </faults>
            </salesforce_prospect>
            <salesforce_product translate="title" module="herfox_salesforce">
                <title>Producto API</title>
                <model>herfox_salesforce/product_api</model>
                <acl>salesforce/product</acl>
                <methods>
                    <list translate="title" module="herfox_salesforce">
                        <title>Retrieve list of productos</title>
                        <method>items</method>
                        <acl>salesforce/product/list</acl>
                    </list>
                    <info translate="title" module="herfox_salesforce">
                        <title>Retrieve producto info</title>
                        <acl>salesforce/product/info</acl>
                    </info>
                    <add translate="title" module="herfox_salesforce">
                        <title>Add producto</title>
                        <acl>salesforce/product/add</acl>
                    </add>
                    <update translate="title" module="herfox_salesforce">
                        <title>Update producto</title>
                        <acl>salesforce/product/update</acl>
                    </update>
                    <remove translate="title" module="herfox_salesforce">
                        <title>Remove producto</title>
                        <acl>salesforce/product/remove</acl>
                    </remove>
                </methods>
                <faults module="herfox_salesforce">
                    <product_not_exists>
                        <code>101</code>
                        <message>Requested producto does not exist.</message>
                    </product_not_exists>
                    <invalid_data>
                        <code>102</code>
                        <message>Provided data is invalid.</message>
                    </invalid_data>
                    <save_error>
                        <code>103</code>
                        <message>Error while saving producto. Details in error message.</message>
                    </save_error>
                    <remove_error>
                        <code>104</code>
                        <message>Error while removing producto. Details in error message.</message>
                    </remove_error>
                </faults>
            </salesforce_product>
            <salesforce_price translate="title" module="herfox_salesforce">
                <title>Precio API</title>
                <model>herfox_salesforce/price_api</model>
                <acl>salesforce/price</acl>
                <methods>
                    <list translate="title" module="herfox_salesforce">
                        <title>Retrieve list of precios</title>
